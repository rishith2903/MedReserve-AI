# MedReserve AI Frontend Environment Configuration

# Backend API Configuration
VITE_API_BASE_URL=https://medreserve-ai-backend.onrender.com
VITE_ML_SERVICE_URL=https://medreserve-ml.onrender.com
VITE_CHATBOT_SERVICE_URL=https://medreserve-chatbot.onrender.com

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://medreserve-ai-backend.onrender.com/ws

# Application Configuration
VITE_APP_NAME=MedReserve AI
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_ML_FEATURES=true

# Development Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# API Timeouts (in milliseconds)
VITE_API_TIMEOUT=30000
VITE_WEBSOCKET_TIMEOUT=10000

# Cache Configuration
VITE_CACHE_DURATION=300000
